        "request": "launch",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "type": "node"
    },
        

        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Downloader",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "console": "externalTerminal"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Account Stats",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/countacc.js",
            "console": "externalTerminal"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Space Comp",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/spacecomp.js",
            "console": "externalTerminal"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Test Store",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/teststore.js",
            "console": "externalTerminal"
